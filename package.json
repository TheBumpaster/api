{
  "name": "api",
  "version": "1.0.0",
  "description": "RESTFull Application with ExpressJS",
  "main": "server/index",
  "scripts": {
    "run": "nodemon",
    "test:help": "mocha --help",
    "test": "mocha --file ./test/util/setup.js --full-trace --colors --diff --exit --timeout 12000 -ui mocha-typescript ./test/**/*.spec.js",
    "migrate:up:run": "npm run build && db-migrate -e dev -m ./dist/services/migrations -v --config ./database.json up",
    "migrate:down:run": "npm run build && db-migrate -e dev -m ./dist/services/migrations -v --config ./database.json down",
    "migrate:create": "db-migrate -e dev -m ./server/services/migrations -v --config ./migrations.json create test_process",
    "build&run": "npm run build && npm run start",
    "build&test": "npm run build && npm run test",
    "build": "tsc --build tsconfig.json --pretty",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheBumpaster/api.git"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "boilerplate",
    "mongodb"
  ],
  "author": "Ismar Hadzic",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/TheBumpaster/api/issues"
  },
  "homepage": "https://github.com/TheBumpaster/api#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.13",
    "db-migrate": "^0.11.11",
    "db-migrate-mongodb": "^1.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "typescript": "^3.8.3",
    "ts-node": "^8.10.1",
    "nodemon": "^2.0.3",
    "mocha": "^7.1.2",
    "mocha-typescript": "^1.1.17",
    "mongodb-memory-server": "^6.5.2",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/chai": "^4.2.11",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/crypto-js": "^3.1.45",
    "@types/dotenv": "^8.2.0",
    "@types/mongoose": "^5.7.16",
    "@types/node": "^13.13.5",
    "@types/express": "^4.17.6",
    "@types/winston": "^2.4.4"
  }
}
